{
  "expo": {
    "name": "shanyai-events-v4",
    "slug": "shanyai-events-v4",
    "scheme": "shanyai-events",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/shanyai-icon.jpg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/shanyai-splash.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "co.event.scanner"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "co.event.scanner"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "react-native-vision-camera",
        {
          "cameraPermissionText": "$(PRODUCT_NAME) needs access to your Camera to scan QR codes.",
          "enableCodeScanner": true
        }
      ]
    ],
  }
}
